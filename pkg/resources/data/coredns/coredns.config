localtest.me.:53 {
        file /etc/coredns/local.db localtest.me {
          upstream
        }
    }

.:53 {
    log
    errors
    health {
      lameduck 5s
    }

    ready
    kubernetes me cluster.local in-addr.arpa ip6.arpa {
      pods insecure
      fallthrough in-addr.arpa ip6.arpa
    }

    rewrite stop {
       name regex (.*)\.localtest.me {1}-ext.default.svc.cluster.local
       answer auto
    }

    prometheus :9153
    forward . "/etc/resolv.conf"
    cache 30
    loop
    reload
    loadbalance
}
